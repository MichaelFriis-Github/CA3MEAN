extends layout
block content
    h2 All Members
    ol
        div.container
            div.col-md-12
                table.table.table-striped.table-hover.table-bordered
                    thead
                        tr
                            th OrderID
                            th ProductID
                            th unitPrice
                            th quantity
                            th discount
                        each s in orders
                            tr
                                td= s.orderID
                                td= s.productID
                                td= s.unitPrice
                                td= s.quantity
                                td= s.discount

                                    orderID,productID,unitPrice,quantity,discount