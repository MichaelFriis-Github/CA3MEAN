extends layout
block content
    h2 All Members
    ol
        div.container
            div.col-md-12
                table.table.table-striped.table-hover.table-bordered
                    tr
                        th ID
                        th Date
                        th Name
                        th Address
                    each s in orders
                        tr
                            td= s.id
                            td= s.orderDate
                            td= s.shipName
                            td= s.shipAddress